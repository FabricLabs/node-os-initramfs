#!/usr/bin/env node

var exists = require('fs').exists

var manager = require('download-manager')


// Source versions

const FUSE_VERSION = "2.9.4"


// Source URLs

const FUSE_URL="http://downloads.sourceforge.net/fuse/fuse-"+FUSE_VERSION+".tar.gz"


//
// FUSE
//

var downloads =
[
  {
    name: 'fuse',
    url: FUSE_URL
  }
]


manager(downloads, function(error)
{
  if(error) throw error;
})
